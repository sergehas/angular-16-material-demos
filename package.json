{
  "name": "angular-16-material-starter",
  "version": "19.2.3-SNAPSHOT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint && npx prettier **/* --write --ignore-unknown",
    "lint:fix": "ng lint --fix",
    "lint:only": "ng lint",
    "pretty": "npx prettier **/* --write --ignore-unknown",
    "clean": "rimraf  node_modules",
    "build:iconlib": "node ./buildIconLib.js",
    "e2e": "ng e2e",
    "e2e:ui": "ng e2e --ui",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.3",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^19.2.3",
    "@angular/compiler": "^19.2.3",
    "@angular/core": "^19.2.3",
    "@angular/forms": "^19.2.3",
    "@angular/material": "^18.2.14",
    "@angular/material-moment-adapter": "^18.2.14",
    "@angular/platform-browser": "^19.2.3",
    "@angular/platform-browser-dynamic": "^19.2.3",
    "@angular/router": "^19.2.3",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "exceljs": "^4.4.0",
    "file-saver": "^2.0.5",
    "rxjs": "~7.8.0",
    "tslib": "^2.6.0",
    "uuid": "^9.0.1",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.2/xlsx-0.20.2.tgz",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.4",
    "@angular/cli": "~19.2.3",
    "@angular/compiler-cli": "^19.2.3",
    "@eslint/js": "^9.22.0",
    "@eslint/json": "^0.10.0",
    "@playwright/test": "^1.50.1",
    "@types/events": "^3.0.3",
    "@types/file-saver": "^2.0.7",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^22.13.8",
    "@types/uuid": "^9.0.8",
    "angular-eslint": "~19.3.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-playwright": "^2.2.0",
    "globals": "^16.0.0",
    "husky": "^9.1.0",
    "jasmine-core": "~5.1.0",
    "jasmine-marbles": "~0.9.2",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.4.3",
    "playwright-ng-schematics": "^1.3.1",
    "prettier": "~3.5.0",
    "rimraf": "^5.0.5",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.26.0"
  },
  "lint-staged": {
    "*.{mjs,json,css,scss,md,ts,html}": [
      "eslint --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": false,
    "quoteProps": "consistent"
  }
}
